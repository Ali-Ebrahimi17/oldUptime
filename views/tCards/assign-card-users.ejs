<!-- new t Card modal -->
<div class="modal fade" id="new-card" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered modal-lg">
		<div class="modal-content">
			<div style="background-color: rgb(252, 176, 38); height: 50px">
				<button style="color: black; margin: 5px" class="close" aria-label="Close"></button>
				<h3 style="text-align: center; margin: 8px">Add New T Card</h3>
			</div>
			<div class="modal-body">
				<form action="/tCard/new/<%= division %>/quality/quality/quality/quality" method="POST">
					<fieldset>
						<div class="form-group">
							<label style="font-weight: bold; color: black" for="name">Name</label>
							<input style="margin-bottom: 8px" class="form-control" type="test" maxlength="19" name="tCard[name]" required />

							<label style="font-weight: bold; color: black" for="exampleTextarea">Pass Requirements</label>
							<textarea
								class="form-control"
								name="tCard[description]"
								id="reasonTextarea"
								rows="8"
								style="resize: none; margin-bottom: 5px"
								required
							></textarea>

							<div class="row" style="margin-bottom: 10px">
								<div class="col">
									<label style="font-weight: bold; color: black" for="name">Method</label>
									<input class="form-control" type="test" maxlength="19" name="tCard[method]" required />
								</div>

								<div class="col">
									<label style="font-weight: bold; color: black" for="name">Frequency</label>
									<select class="form-control" type="text" name="tCard[frequency]" autocomplete="off" required>
										<option hidden></option>
										<option>Daily</option>
										<option>Weekly</option>
										<option>Monthly</option>
										<option>Quarterly</option>
									</select>
								</div>
							</div>
						</div>
						<div class="row" style="margin-bottom: 10px">
							<div class="col">
								<label style="font-weight: bold; color: black" for="type" id="type">Check Type</label>
								<select class="form-control" id="type" name="tCard[type]" id="completedBy" required>
									<option hidden></option>
									<option>Value</option>
									<option>OK/Not OK</option>
									<option>Yes/No</option>
									<option>On Plan/Off Plan</option>
								</select>
							</div>

							<div class="col">
								<label style="font-weight: bold; color: black" for="type" id="type">Check Level</label>
								<select class="form-control" id="level" name="tCard[level]" id="level" required>
									<option hidden></option>
									<option>QC</option>
									<option>CTQ</option>
								</select>
							</div>
						</div>

						<div class="row" style="margin-bottom: 10px">
							<div class="col">
								<label style="font-weight: bold; color: black" for="type" id="type">Check Section</label>
								<select class="form-control" id="type" name="tCard[section]" id="completedBy" required>
									<option hidden></option>
									<option>Assembly</option>
									<option>Paint</option>
									<option>Plant Quality</option>
									<option>Manufacturing</option>
								</select>
							</div>

							<div class="col">
								<label style="font-weight: bold; color: black" for="type" id="type">Check Category</label>
								<select class="form-control" id="level" name="tCard[category]" id="level" required>
									<option hidden></option>
									<option>Equipment</option>
									<option>Process</option>
									<option>People</option>
									<option>Adherence to Standards</option>
								</select>
							</div>
						</div>

						<div class="row">
							<div class="col">
								<label style="font-weight: bold; color: black" for="exampleSelect1">Manual Target</label>
								<select style="margin-bottom: 15px" class="form-control" name="tCard[manual]" id="completedBy" required>
									<option hidden></option>
									<option>Yes</option>
									<option>No</option>
								</select>
							</div>
							<div class="col">
								<label style="font-weight: bold; color: black" for="name" id="nim">Min</label>
								<input class="form-control" type="number" step="0.01" name="tCard[min]" />
							</div>
							<div class="col">
								<label style="font-weight: bold; color: black" for="name" id="max">Max</label>
								<input class="form-control" type="number" step="0.01" name="tCard[max]" />
							</div>
							<div class="col">
								<label style="font-weight: bold; color: black" for="name" id="max">Units</label>
								<input class="form-control" style="margin-bottom: 15px" type="text" maxlength="5" name="tCard[unit]" required />
							</div>
						</div>

						<button
							type="button"
							id="modalClose"
							data-dismiss="modal"
							class="btn btn-danger float-right"
							style="margin-left: 8px; color: white; border-radius: 4px"
						>
							Cancel
						</button>
						<button type="submit" class="btn btn-success float-right" style="margin-left: 8px">Submit</button>
					</fieldset>
				</form>
			</div>
			<div style="background-color: rgb(252, 176, 38); display: flex; align-items: center; height: 50px; display: flex">
				<img style="margin: 5px" height="24px" src="/7z/content/images/jcb-logo.png" /> <img height="24px" src="/7z/content/images/df.png" />
			</div>
		</div>
	</div>
</div>

<!-- new User modal -->
<div class="modal fade" id="new-user" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered modal-md">
		<div class="modal-content">
			<div style="background-color: rgb(252, 176, 38); height: 50px">
